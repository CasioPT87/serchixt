{
  "name": "genei-front",
  "version": "1.0.0",
  "description": "Genei front end client",
  "main": "server.js",
  "scripts": {
    "debug-front": "webpack-dev-server --open --mode development",
    "debug-back": "npm run delete-dist && nodemon --watch 'src/**' --ext 'ts,tsx,js,json' --exec 'webpack --mode development'",
    "dev": "concurrently \"npm run debug-back\" \"npm run debug-front\"",
    "start": "npm run delete-transp && tsc && webpack --mode production && node ./transpiled/src/app.js",
    "test-dev": "concurrently \"npm run dev\" \"sleep 5 && jest dev/\"",
    "test-prod": "NODE_ENV=production concurrently \"npm start\" \"sleep 5 && jest prod/\"",
    "delete-dist": "rm -rf ./src/dist",
    "delete-transp": "rm -rf ./transpiled"
  },
  "author": "Geneit dev team",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/node": "^7.22.19",
    "@babel/preset-react": "^7.23.3",
    "@redux-devtools/extension": "^3.3.0",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-redux": "^7.1.33",
    "babel": "^6.23.0",
    "babel-loader": "^9.1.3",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "redux-thunk": "^3.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/register": "^7.23.7",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^21.10.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack-dev-server": "^4.15.1"
  }
}
