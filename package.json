{
  "name": "genei-front",
  "version": "1.0.0",
  "description": "Genei front end client",
  "main": "server.js",
  "scripts": {
    "dev": "npm run delete-files && webpack-dev-server --open --env bundle=development",
    "start": "npm run delete-files && webpack --env bundle=production && webpack --env bundle=production-server && node ./src/dist-server/bundle-server.js",
    "dev-server": "npm run delete-files && webpack --env bundle=production && webpack --env bundle=development-server && node ./src/dist-server/bundle-server.js",
    "test": "./run-tests.sh",
    "delete-files": "rm -rf ./src/dist && rm -rf ./src/dist-server",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "author": "Geneit dev team",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "axios": "^1.7.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "redux-thunk": "^3.1.0",
    "source-map-loader": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@redux-devtools/extension": "^3.3.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.33",
    "css-loader": "^7.1.2",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "prettier": "^3.3.3",
    "puppeteer": "^21.10.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
