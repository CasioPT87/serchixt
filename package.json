{
  "name": "genei-front",
  "version": "1.0.0",
  "description": "Genei front end client",
  "main": "server.js",
  "scripts": {
    "debug-front": "webpack-dev-server --open --mode development",
    "debug-back": "npm run delete-dist && nodemon --watch 'src/**' --ext 'ts,tsx,js,json' --exec 'webpack --mode development'",
    "dev": "concurrently \"npm run debug-back\" \"npm run debug-front\"",
    "start": "npm run delete-transp && tsc && cp -r ./src/styles ./transpiled/src/styles && webpack --mode production && webpack --mode production --config webpack.config.server.cjs  && node ./src/dist-server/bundle-server.js",
    "test-dev": "concurrently \"npm run dev\" \"sleep 5 && jest dev/\"",
    "test-prod": "NODE_ENV=production concurrently \"npm start\" \"sleep 5 && jest prod/\"",
    "delete-dist": "rm -rf ./src/dist",
    "delete-transp": "rm -rf ./transpiled"
  },
  "author": "Geneit dev team",
  "license": "ISC",
  "dependencies": {
    "-": "^0.0.1",
    "@redux-devtools/extension": "^3.3.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-redux": "^7.1.33",
    "axios": "^1.7.2",
    "cookie-parser": "^1.4.6",
    "D": "^1.0.0",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "redux-thunk": "^3.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "puppeteer": "^21.10.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  }
}
